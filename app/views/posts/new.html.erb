新規投稿

<% if @error_message %>
<div class="form-error">
  <%= @error_message %>
</div>
<% end %>

<%= form_with(url: "/posts") do %>
<p>タイトル</p>
<input name="title" value="<%= @post.title %>">
<p>内容</p>
<input name="content" value="<%= @post.content %>">
<p>コード</p>
<input name="code" value="<%= @post.code %>">
<input type="submit" value="新規投稿">
<% end %>
